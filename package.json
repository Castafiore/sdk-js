{
  "name": "@directus/sdk-js",
  "version": "5.2.1",
  "description": "Directus SDK for JavaScript (Node and Browser)",
  "main": "dist/directus-sdk.js",
  "browser": "dist/directus-sdk.umd.min.js",
  "types": "dist/types/src/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prettier": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"**/*.json\"",
    "develop": "webpack --progress --colors --watch",
    "prebuild": "npm-run-all test",
    "build": "run-s build:*",
    "build:webpack": "webpack",
    "build:declaration": "tsc --project ./tsconfig.meta.json",
    "test": "run-s test:*",
    "test:lint": "tslint --project ./tslint.json",
    "test:mocha": "ts-mocha -p ./tsconfig.test.json test/**/*.spec.ts --exit --recursive"
  },
  "repository": "directus/sdk-js",
  "keywords": [
    "node",
    "javascript",
    "directus",
    "headless",
    "cms",
    "sdk",
    "client",
    "api"
  ],
  "author": "RANGER Studio LLC",
  "contributors": [
    "Rijk van Zanten <rijkvanzanten@me.com>",
    "Jan Biasi <biasijan@gmail.com>"
  ],
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/directus/directus-sdk-javascript/issues"
  },
  "homepage": "https://docs.directus.io/sdk/js.html",
  "dependencies": {
    "argument-validator": "0.1.0",
    "axios": "0.18.0",
    "base-64": "0.1.0",
    "node": "11.13.0",
    "qs": "6.7.0"
  },
  "devDependencies": {
    "@types/expect": "1.20.4",
    "@types/mocha": "5.2.6",
    "@types/qs": "6.5.3",
    "chai": "4.2.0",
    "chai-datetime": "1.5.0",
    "chalk": "2.4.2",
    "jsonwebtoken": "8.5.1",
    "mocha": "6.1.4",
    "npm-run-all": "4.1.5",
    "nyc": "14.0.0",
    "prettier": "1.17.0",
    "rimraf": "2.6.3",
    "sinon": "7.3.2",
    "sinon-chai": "3.3.0",
    "ts-loader": "5.3.3",
    "ts-mocha": "6.0.0",
    "tslib": "1.9.3",
    "tslint": "5.16.0",
    "typescript": "3.4.4",
    "webpack": "4.30.0",
    "webpack-bundle-analyzer": "3.3.2",
    "webpack-cli": "3.3.0",
    "webpack-node-externals": "1.7.2"
  }
}
